# Begin Podfile - /Users/user934491/Documents/HeliarHexpress.APPLojista/app/App_Resources/iOS/Podfile
# platform :ios, '11'

def post_installNSPodfileBase_0 (installer)
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '11'
    end
    target.build_configurations.each do |config|
      config.build_settings['EXCLUDED_ARCHS[sdk=iphonesimulator*]'] = 'uname -m'
    end
    # Make it work with GoogleDataTransport
    if target.name.start_with? "GoogleDataTransport"
      target.build_configurations.each do |config|
        config.build_settings['CLANG_WARN_STRICT_PROTOTYPES'] = 'NO' 
      end
    end
  end
end
# End Podfile
end